annotation_north_arrow(location = "tr", which_north = "true",   #放置指北針
pad_x = unit(0.2, "cm"), pad_y = unit(0.6, "cm"),
height = unit(1.0, "cm"), width = unit(1.0, "cm") ,
style = north_arrow_fancy_orienteering(text_col = 'grey10',
line_col = 'grey10',
fill = 'grey10'))+
coord_sf(xlim = c(119.8, 122.8), ylim = c(21.8, 25.5), expand = FALSE, crs=4326)+
theme(#panel.border = element_rect(colour = "black", fill=NA, size=1.0),
panel.background = element_rect(fill = "#003b4f"),
panel.grid = element_blank(),
plot.margin = margin(0, 0, -0.4, -0.4, "cm"),
axis.text.x = element_text(vjust = 8, size=8),
axis.text.y = element_text(angle = 270, vjust = 10, hjust=0.5, size=8),
#axis.text.x = element_blank(),
#axis.text.y = element_blank(),
axis.ticks.x = element_blank(),
axis.title.x = element_blank(),
axis.ticks.y = element_blank(),
axis.title.y = element_blank(),
legend.position="none")
ggexport(RiverTaiwan, filename = "Taiwan River map.png", width =5000, height =6670, res =900 )
